<div class="container body-content">

    <h1>@ViewBag.title</h1>

    
    <!--//TODO #1 Create Loop-->



        @foreach (Job job in ViewBag.jobs)
        {
            <table class="Table">
                <tr>
                    <th>Id:</th>
                    <th>@job.Id</th>
                </tr>
                <tr>
                    <th>Name:</th>
                    <th>@job.Name</th>
                </tr>
                <tr>
                    <th>Id:Employer</th>
                    <th>@job.Employer</th>
                </tr>
                <tr>
                    <th>Location:</th>
                    <th>@job.Location</th>
                </tr>
                <tr>
                    <th>Position Type:</th>
                    <th>@job.PositionType</th>
                </tr>
                <tr>
                    <th>Core Competency:</th>
                    <th>@job.CoreCompetency</th>
                </tr>
            </table>
            <p>-------------------------------------------------------------------------------</p>


        }
</div>
